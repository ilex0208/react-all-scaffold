{
  "name": "ilex",
  "version": "0.0.1",
  "description": "ilex with react",
  "main": "index.js",
  "author": "ilex",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack -p --progress --colors",
    "watch": "webpack -w",
    "build:single":"webpack --display-error-details",
    "start": "webpack-dev-server --progress --colors --port 9000 --inline --content-base .",
    "start2": "webpack-dev-server --progress --colors --port 9000 --inline --hot --quiet",
    "start3": "webpack-dev-server --progress --colors --port 9000 --host 0.0.0.0 --inline --content-base .",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "dev": "better-npm-run deploy:dev",
    "prod": "better-npm-run deploy:prod",
    "miniServer": "better-npm-run miniServer",
    "node": "better-npm-run node",
    "node:prod": "better-npm-run node:prod"
  },
  "betterScripts": {
    "miniServer": {
      "command": "node miniServer",
      "env": {"DEBUG": "app:*"}
    },
    "node": {
      "command": "babel-node bin/server",
      "env": {"DEBUG": "app:*"}
    },
    "node:prod": {
      "command": "babel-node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "babel-node bin/compile",
      "env": {"DEBUG": "app:*"}
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {"DEBUG": "app:*"}
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
